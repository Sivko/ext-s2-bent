{
  "name": "Zillow Reader",
  "description": "Read zillow data",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "activeTab"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["contentscript.js"],
    "css": ["style.css"]
  }]
}