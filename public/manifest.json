{
  "name": "S2 - extbent",
  "description": "Read zillow data",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://app.salesap.ru/*",
    "https://partners.dasreda.ru/*"
  ],
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script_crm.js",
        "script_dasreda.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}